<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="org_gephi_branding_update_center.instance">
                <attr name="displayName" bundlevalue="org.gephi.branding.Bundle#Services/AutoupdateType/org_gephi_branding_update_center.instance"/>
                <attr name="enabled" boolvalue="${enableAutoUpdate}"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" stringvalue="${gephi.update.center.official.url}"/>
            </file>
            <file name="org_gephi_branding_update_center_1.instance">
                <attr name="displayName" bundlevalue="org.gephi.branding.Bundle#Services/AutoupdateType/org_gephi_branding_update_center_1.instance"/>
                <attr name="enabled" boolvalue="${enableAutoUpdate}"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" stringvalue="${gephi.update.center.thirdparty.url}"/>
            </file>
        </folder>
    </folder>
</filesystem>
